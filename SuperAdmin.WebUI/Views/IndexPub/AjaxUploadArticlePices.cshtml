@model SuperAdmin.WebUI.Models.FileUploadViewModel
<div class="form-group">
    <form id="uploadform" role="form" method="post" enctype="multipart/form-data" >
        @Html.TextBoxFor(m => m.File, new { type = "file", @class = "file" })
        <br>
        <button type="submit" class="btn btn-primary" id="uploadbtn1" >提交</button>
       </form>
</div>