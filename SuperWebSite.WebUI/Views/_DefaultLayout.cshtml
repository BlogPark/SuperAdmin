<!DOCTYPE html>
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="keyword" content="@ViewBag.keyword">
    <!-- Mobile Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Favicon -->
    <link rel="shortcut icon" href="/images/favicon.ico">
    <!-- Web Fonts -->
    <!-- Bootstrap core CSS -->
    <link href="/bootstrap/css/bootstrap.css" rel="stylesheet">
    <!-- Font Awesome CSS -->
    <link href="/fonts/font-awesome/css/font-awesome.css" rel="stylesheet">
    <!-- Fontello CSS -->
    <link href="/fonts/fontello/css/fontello.css" rel="stylesheet">
    <!-- Plugins -->
    <link href="@Url.Content("/plugins/rs-plugin/css/settings.css")" media="screen" rel="stylesheet">
    <link href="@Url.Content("/plugins/rs-plugin/css/extralayers.css")" media="screen" rel="stylesheet">
    <link href="@Url.Content("/plugins/magnific-popup/magnific-popup.css")" rel="stylesheet">
    <link href="@Url.Content("/css/animations.css")" rel="stylesheet">
    <link href="@Url.Content("/plugins/owl-carousel/owl.carousel.css")" rel="stylesheet">
    <!-- iDea core CSS file -->
    <link href="@Url.Content("/css/style.css")" rel="stylesheet">
    <!-- Color Scheme (In order to change the color scheme, replace the red.css with the color scheme that you prefer)-->
    <link href="@Url.Content("/css/skins/red.css")" rel="stylesheet">
    <!-- Custom css 
		<link href="/css/custom.css" rel="stylesheet">-->
    @RenderSection("mycss", required: false)
    @*<!-–[if lt IE 9]-->
		<script type="text/javascript"  src="js/html5shiv.js"></script >
		<script type="text/javascript" src="js/selectivizr.js"></script>
		<!--[endif]–- > *@

</head>

<!-- body classes: 
			"boxed": boxed layout mode e.g. <body class="boxed">
			"pattern-1 ... pattern-9": background patterns for boxed layout mode e.g. <body class="boxed pattern-1"> 
	-->
<body class="front">
    <!-- scrollToTop -->
    <!-- ================ -->
    <div class="scrollToTop"><i class="icon-up-open-big"></i></div>
    
    <!-- page wrapper start -->
    <!-- ================ -->
    <div class="page-wrapper">

        <!-- header-top start -->
        <!-- ================ -->
        @Html.Action("PageTop", "DivisionArea", new { area = "" })
        <!-- header-top end -->

        <!-- header start (remove fixed class from header in order to disable fixed navigation mode) -->
        <!-- ================ -->
        @Html.Action("PageNav", "DivisionArea", new { area = "" })
        <!-- header end -->

        <!--页面主体开始-->
        @RenderBody()        
        <!--页面主体结束-->

        <!-- footer start (Add "light" class to #footer in order to enable light footer) -->
        <!-- ================ -->
        <footer id="footer">

            <!-- .footer start -->
            <!-- ================ -->
            @Html.Action("DefaultFooter", "DivisionArea", new { area = "" })          
            <!-- .footer end -->

            <!-- .subfooter start -->
            <!-- ================ -->
          @Html.Action("PageSubFooter", "DivisionArea", new { area = "" })
            <!-- .subfooter end -->

        </footer>
        <!-- footer end -->

    </div>
    <!-- page-wrapper end -->
    <!-- JavaScript files placed at the end of the document so the pages load faster
		================================================== -->
    <!-- Jquery and Bootstap core js files -->
    <script type="text/javascript" src="@Url.Content("/plugins/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("/bootstrap/js/bootstrap.min.js")"></script>
    <!-- Modernizr javascript -->
    <script type="text/javascript" src="@Url.Content("/plugins/modernizr.js")"></script>
    <!-- jQuery REVOLUTION Slider  -->
    <script type="text/javascript" src="@Url.Content("/plugins/rs-plugin/js/jquery.themepunch.tools.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("/plugins/rs-plugin/js/jquery.themepunch.revolution.min.js")"></script>
    @RenderSection("scriptfiles", required: false)
    <!-- Owl carousel javascript -->
    <script type="text/javascript" src="@Url.Content("/plugins/owl-carousel/owl.carousel.js")"></script>
    <!-- Magnific Popup javascript -->
    <script type="text/javascript" src="@Url.Content("/plugins/magnific-popup/jquery.magnific-popup.min.js")"></script>
    <!-- Appear javascript -->
    <script type="text/javascript" src="@Url.Content("/plugins/jquery.appear.js")"></script>
    <!-- Initialization of Plugins -->
    <script type="text/javascript" src="@Url.Content("/js/template.js")"></script>
    <!-- Isotope javascript 
		<script type="text/javascript" src="/plugins/isotope/isotope.pkgd.min.js"></script>-->
    <!-- Count To javascript 
		<script type="text/javascript" src="/plugins/jquery.countTo.js"></script>-->
    <!-- Parallax javascript 
		<script src="/plugins/jquery.parallax-1.1.3.js"></script>-->
    <!-- Contact form
		<script src="/plugins/jquery.validate.js"></script> -->

    <!-- Custom Scripts 
		<script type="text/javascript" src="/js/custom.js"></script>-->
    @RenderSection("myscript", required: false)
</body>
</html>
